#include<stdio.h>
int large(int a, int b){
  (a > b) ? return a : return b;
}
int main(){
  int num1,num2,large;
  printf("Enter two number: ");
  scanf("%d%d",&num1,&num2);
  if(num1 == num2)
    print("Both are equal");
  else{
    large = large(num1,num2);
    print("Largest number is ", large);
  }
  return 0;
}
